<% if (blogs.length> 0) { %>

    <h2 class="mt-2">Latest posts</h2>

    <div class="table-responsive">
        <table class="table table-striped table-sm">
            <thead>
                <tr>
                    <th>
                        #
                    </th>
                    <th>title</th>
                    <th>status</th>
                    <th>Date</th>
                </tr>
            </thead>

            <% blogs.forEach(function(item,index) {%>
                <tbody>
                    <tr>
                        <td>1</td>
                        <td>
                            <%= item.title %>
                        </td>
                        <td>
                            <span type="button" class="btn btn-sm <%= item.status === "public" ? 'btn-success' : 'btn-secondary'
                            %>">
                            <%= item.status %>
                        </span>
                    </td>
                    <td>
                        <%= formatDate(item.createdAt) %>
                    </td>
                    <td>
                        <a href="/dashboard/edit-post/<%= item._id %>" class="btn btn-sm btn-primary" >Edit</a>
                        <a href="/dashboard/delete-post/<%= item._id %>" class="btn btn-sm btn-danger" >Delete</a>
                    </td>
                    </tr>
                </tbody>
                <% }) %>



        </table>
    </div>



    <% } else { %>
        <div class="text-center">
            <h3 class="mt-2">
                there is not any post
            </h3>
        </div>
        <% } %>